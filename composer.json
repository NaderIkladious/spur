{
    "name": "spur/spur",
    "description": "A Laravel driver for spur to on the fly get required components",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Spur\\Spur\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spur\\Spur\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nader Ikladious",
            "email": "nader.ikladious@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Spur\\Spur\\Providers\\SpurServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.15"
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    }
}
