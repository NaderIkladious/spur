{
    "name": "spur/spur",
    "description": "A Laravel driver for spur to on the fly get required components",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spur\\Spur\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spur\\Spur\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nader Ikladious",
            "email": "nader.ikladious@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Spur\\Spur\\Providers\\SpurServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
